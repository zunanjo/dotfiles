{
    "layer": "top",
    "position": "top",
    "height": 50,
    "modules-left": [
      "hyprland/workspaces",
      "memory",
      "disk",
      "cpu",
      "temperature"
    ],
    "fixed-center": true,
    "modules-center": [
      "clock"
    ],
    "modules-right": [
      "network",
      "bluetooth",
      "hyprland/language",
      "backlight",
      "pulseaudio",
      "battery"
    ],
    "temperature": {
      "critical-threshold": 70,
      "format": "{icon} {temperatureC}Â°C",
      "format-icons": ["ï«", "ï‹‰", "ï©"],
      "tooltip": false
    },
    "cpu": {
      "format": "ï‹› {usage}%",
